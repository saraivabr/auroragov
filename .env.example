# Aurora Gov - Environment Variables Template
# Copy this file to .env and fill in your values

# ==========================================
# SUPABASE CONFIGURATION
# ==========================================
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# ==========================================
# AI MODEL API KEYS
# ==========================================
# OpenAI (ChatGPT)
VITE_OPENAI_API_KEY=your_openai_api_key

# Anthropic (Claude)
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key

# Google AI (Gemini)
VITE_GOOGLE_API_KEY=your_google_api_key

# DeepSeek
VITE_DEEPSEEK_API_KEY=your_deepseek_api_key

# ==========================================
# APPLICATION CONFIGURATION
# ==========================================
# Base path for deployment (leave as "/" for root)
VITE_BASE_PATH=/

# Environment (development, staging, production)
NODE_ENV=development

# ==========================================
# SECURITY CONFIGURATION
# ==========================================
# Session timeout in minutes (default: 45)
VITE_SESSION_TIMEOUT=45

# Enable audit logging (true/false)
VITE_ENABLE_AUDIT_LOG=true

# ==========================================
# OPTIONAL: MONITORING & ANALYTICS
# ==========================================
# Sentry DSN for error tracking
VITE_SENTRY_DSN=

# Google Analytics ID
VITE_GA_ID=

# ==========================================
# OPTIONAL: FEATURE FLAGS
# ==========================================
VITE_ENABLE_COMPARISON_MODE=true
VITE_ENABLE_DOCUMENT_WORKSPACE=true
VITE_ENABLE_ONBOARDING_TOUR=true
